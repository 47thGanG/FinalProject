<?xml version='1.1' encoding='UTF-8'?>
<project>
  <actions/>
  <description>This is Final Project. This Job runs the hello.sh script from github</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <com.coravy.hudson.plugins.github.GithubProjectProperty plugin="github@1.36.1">
      <projectUrl>https://github.com/47thGanG/FinalProject.git/</projectUrl>
      <displayName></displayName>
    </com.coravy.hudson.plugins.github.GithubProjectProperty>
  </properties>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@5.0.0">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>https://github.com/47thGanG/FinalProject.git</url>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/main</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="empty-list"/>
    <extensions/>
  </scm>
  <assignedNode>slave</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>echo &apos;---------------Build Started---------------&apos;
cd /home/srbektimirov/workspace/HelloPY+Minikube+Grafana/
mv -f Dockerfile db-deployment.yaml hello.py hello.sh requirements.txt secret.yaml web-deployment.yaml node-exporter.service cadvisor.service blackbox-exporter.service alertmanager.yml alertmanager.service prometheus.yml mainp.yml prometheus.service maing.yml maing2.yml grafana.service /home/srbektimirov/
cd /home/srbektimirov
chmod +x hello.sh
./hello.sh
echo &apos;---------------Build Finished---------------&apos;</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>echo &apos;---------------Test Started---------------&apos;
echo &apos;Your tests could be here&apos;
echo &apos;---------------Test Finished---------------&apos;</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.BuildTrigger>
      <childProjects>Proxy</childProjects>
      <threshold>
        <name>SUCCESS</name>
        <ordinal>0</ordinal>
        <color>BLUE</color>
        <completeBuild>true</completeBuild>
      </threshold>
    </hudson.tasks.BuildTrigger>
  </publishers>
  <buildWrappers/>
</project>
